{
    "name": "sonata-project/user-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataUserBundle",
    "keywords": ["user", "sonata", "google authenticator"],
    "homepage": "http://sonata-project.org/bundles/user",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "http://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataUserBundle/contributors"
        }
    ],
    "require": {
        "symfony/http-foundation": ">=2.1,<2.3-dev",
        "symfony/form": ">=2.1,<2.3-dev",
        "symfony/security": ">=2.1,<2.3-dev",
        "symfony/console": ">=2.1,<2.3-dev",
        "sonata-project/admin-bundle": "2.1.*@dev",
        "friendsofsymfony/user-bundle": "2.0.*@dev",
        "sonata-project/doctrine-extensions": "1.*",
        "sonata-project/easy-extends-bundle": "2.1.*",
        "sonata-project/google-authenticator": "1.*"
    },
    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "2.1.*@dev"
    },
    "autoload": {
        "psr-0": { "Sonata\\UserBundle": "" }
    },
    "target-dir": "Sonata/UserBundle",
    "extra": {
        "branch-alias": {
            "dev-2.0": "2.0.x-dev",
            "dev-master": "2.1.x-dev"
        }
    }
}
